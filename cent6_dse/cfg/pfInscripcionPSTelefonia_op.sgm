<! BBVA  ---------------------------------------------------------------------------->
<! Operación:  PRESENTACION pfInscripcionPSTelefonia_op  ------------------------------------->

<! OPERACION ----------------------------------------------------------------------->
<com.grupobbva.ce.op.pf.OPInscripcionPSTelefonia id=pfInscripcionPSTelefonia_op operationContext=OperationContext>
	<refFormat name=jspFormat refId=ErrorOPJspFmt>	
	<refFormat name=jspFormat refId=InscripcionPSTelefoniaOPJspFmt>	
	<refFormat name=htmlRequestFormat refId=InscripcionPSTelefoniaOPHTMLFmt>
	<refKColl refId=pfInscripcionPSTelefonia_op-data>
	
<! --------------------------------------------------------------------------------->

<! ESTADOS --------------------------------------------------------------------------->
	<navegacionOP>	
		<estadoOP id=0 accion=inscribir metodo=inscribir requestFmt=InscripcionPSTelefoniaOPHTMLFmt>
		<estadoOP id=1 accion=inscribir metodo=inscribir requestFmt=InscripcionPSTelefoniaOPHTMLFmt>
		<estadoOP id=ERROR accion=inscribir metodo=inscribir requestFmt=InscripcionPSTelefoniaOPHTMLFmt>
	</navegacionOP>		
	
	<! SALIDAS --------------------------------------------------------------------------->
	<resultadoOP>	
		<salidaOP id=ERROR jspFmt=ErrorOPJspFmt>				
		<salidaOP id=1 jspFmt=InscripcionPSTelefoniaOPJspFmt>
	</resultadoOP>	
</com.grupobbva.ce.op.pf.OPInscripcionPSTelefonia>

<! DATOS --------------------------------------------------------------------------->
<kColl id=pfInscripcionPSTelefonia_op-data>
	<field id=Error>
	<field id=Servicio>
	<field id=Operacion>
	<field id=UsuarioAutoriza>
	<field id=NumCuentaCargo>
	<field id=MonCuentaCargo>
	<field id=NumCuentaAbono>
	<field id=MonCuentaAbono>
	<field id=TitCuentaAbono>
</kColl>
		
<! FORMATOS --------------------------------------------------------------------------->
<fmtDef id=ErrorOPJspFmt>
	<fJsp>
		<fString dataName=s_cod_nomemp description=empresa><nullCheck>
		<fString dataName=s_cod_nomrep description=usuario><nullCheck>
		<fString dataName=s_fecha_host description=fecha_host>
		<fString dataName=s_hora_host description=hora_host>
		<fString dataName=Error description=mensaje>
	</fJsp>
</fmtDef>

<fmtDef id=InscripcionPSTelefoniaOPJspFmt>
	<fJsp>

		<fString dataName=s_fecha_host description=fecha_host>
		<fString dataName=s_hora_host description=hora_host>

		<fString dataName=s_cod_nomemp description=empresa><nullCheck>
		<fString dataName=s_cod_nomrep description=usuario><nullCheck>
		
		<fString dataName=prServicio description=servicio>
		<fString dataName=prNumeroCuentaCargo description=numeroCuentaCargo>
		<fString dataName=prNumeroSuministro description=numeroSuministro>
		<fString dataName=prTitularServicio description=titularServicio>
		<fString dataName=prImportePagado description=importePagado>
		<fString dataName=prUsuariosAutorizan description=usuariosAutorizan>
		
	</fJsp>	
</fmtDef>


<fmtDef id=InscripcionPSTelefoniaOPHTMLFmt>
	<fHTMLReq>		
		<fString dataName=Operacion>
		<fString dataName=UsuarioAutoriza>
		<fString dataName=NumCuentaCargo>
		<fString dataName=MonCuentaCargo>
		<fString dataName=NumCuentaAbono>
		<fString dataName=MonCuentaAbono>		
		<fString dataName=TitCuentaAbono>
	</fHTMLReq>
</fmtDef>	


